{
	"name": "sarif",
	"publisher": "Jeff",
	"displayName": "SARIF",
	"description": "SARIF for Visual Studio Code",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/sarif-vscode-extension.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/context.js",
	"contributes": {
		"commands": [
			{
				"command": "sarif.showResultsPanel",
				"title": "Show SARIF Panel"
			}
		]
	},
	"scripts": {
		"start": "webpack-dev-server --mode development & webpack --watch",
		"test": "mocha",
		"vscode:prepublish": "webpack",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/mocha": "2.2.48",
		"@types/node": "^10.12.21",
		"@types/react": "16.9.26",
		"@types/react-dom": "16.9.5",
		"@types/sarif": "2.1.2",
		"css-loader": "3.4.2",
		"mocha": "7.1.1",
		"mock-require": "3.0.3",
		"node-sass": "4.13.1",
		"sass-loader": "8.0.2",
		"style-loader": "1.1.3",
		"ts-loader": "6.2.2",
		"ts-node": "8.8.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"url-loader": "4.0.0",
		"vscode": "^1.1.28",
		"webpack": "4.42.1",
		"webpack-cli": "3.3.11",
		"webpack-dev-server": "3.10.3"
	},
	"dependencies": {
		"mobx": "5.15.4",
		"mobx-react": "6.1.8",
		"react": "16.13.1",
		"react-dom": "16.13.1"
	}
}
