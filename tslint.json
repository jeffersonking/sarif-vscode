{
	"rules": {
		"class-name": true,
		"no-console": [true, "log"],
		"no-debugger": true,
		"no-string-throw": true,
		"no-var-keyword": true,
		"prefer-const": true,

		"indent": [true, "tabs"],
		"no-trailing-whitespace": true,
		"quotemark": [true, "single", "jsx-double"],
		"semicolon": [true, "never"],
		"triple-equals": true
	},
	"defaultSeverity": "warning"
}
